<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondop"
    tools:context=".EstatusActivity">

    <TextView
        android:id="@+id/tvidcomp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:minWidth="105dp"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        android:text="Comprobante:"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="16sp"
        tools:ignore="RtlCompat"></TextView>

    <TextView
        android:id="@+id/tvfecha2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvidcomp2"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="@android:color/transparent"
        android:minWidth="105dp"
        android:paddingStart="2dp"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        android:text="Fecha:"
        android:textColor="@color/white"
        android:textSize="16sp"></TextView>

    <TextView
        android:id="@+id/tvmoned2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvfecha2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="@android:color/transparent"
        android:minWidth="105dp"
        android:paddingStart="2dp"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        android:text="Moneda:"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="16sp"></TextView>

    <TextView
        android:id="@+id/tvoficd2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvmoned2"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="@android:color/transparent"
        android:minWidth="105dp"
        android:paddingStart="2dp"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        android:text="Of. Destino:"
        android:textColor="@color/white"
        android:textSize="16sp"></TextView>

    <TextView
        android:id="@+id/tvmonto2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvoficd2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="@android:color/transparent"
        android:minWidth="105dp"
        android:paddingStart="2dp"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        android:text="Monto:"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="16sp"></TextView>

    <TextView
        android:id="@+id/tvestatus2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvmonto2"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="17dp"
        android:gravity="center"
        android:minWidth="105dp"
        android:paddingStart="3dp"
        android:paddingEnd="3dp"
        android:text="Estatus:"
        android:textAlignment="textStart"
        android:textColor="@color/white">

    </TextView>

    <TextView
        android:id="@+id/tvestatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvmonto"
        android:layout_alignParentLeft="false"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="17dp"
        android:layout_toRightOf="@+id/tvestatus2"
        android:gravity="center"
        android:minWidth="70dp"
        android:paddingStart="3dp"
        android:paddingEnd="3dp"
        android:text="Aprobado"
        android:textAlignment="textStart"
        android:textColor="@color/white">

    </TextView>

    <TextView
        android:id="@+id/tvidcomp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="false"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_toRightOf="@+id/tvidcomp2"
        android:gravity="center"
        android:minWidth="70dp"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        android:text="1"
        android:textAlignment="textEnd"
        android:textColor="@color/white"
        android:textSize="16sp"
        tools:ignore="RtlCompat"></TextView>

    <TextView
        android:id="@+id/tvfecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvidcomp"
        android:layout_alignParentLeft="false"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_toRightOf="@+id/tvfecha2"
        android:background="@android:color/transparent"
        android:minWidth="70dp"
        android:paddingStart="2dp"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        android:text="Fecha"
        android:textColor="@color/white"
        android:textSize="16sp"></TextView>

    <TextView
        android:id="@+id/tvmoned"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvfecha"
        android:layout_alignParentStart="false"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_toRightOf="@+id/tvmoned2"
        android:background="@android:color/transparent"
        android:minWidth="70dp"
        android:paddingStart="2dp"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        android:text="Moneda"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="16sp"></TextView>

    <TextView
        android:id="@+id/tvoficd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvmoned"
        android:layout_alignParentLeft="false"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_toRightOf="@+id/tvoficd2"
        android:background="@android:color/transparent"
        android:minWidth="70dp"
        android:paddingStart="2dp"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        android:text="OficD"
        android:textColor="@color/white"
        android:textSize="16sp"></TextView>

    <TextView
        android:id="@+id/tvmonto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvoficd"
        android:layout_alignParentStart="false"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_toRightOf="@+id/tvmonto2"
        android:background="@android:color/transparent"
        android:minWidth="70dp"
        android:paddingStart="2dp"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        android:text="Monto"
        android:textAlignment="textEnd"
        android:textColor="@color/white"
        android:textSize="16sp"></TextView>

</RelativeLayout>